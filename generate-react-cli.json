{
  "usesTypeScript": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/App/Components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "hookedFunc": {
      "customTemplates": {
        "component": "grc_templates/funcWithState/TemplateName.js"
      },
      "path": "src/App/Components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "layoutFunc": {
      "customTemplates": {
        "component": "grc_templates/funcWithLayout/TemplateName.js"
      },
      "path": "src/App/Components/layouts",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    }
  }
}
